{
  "TrainGenotypeFilteringModel.vcf": {{ test_batch.concordance_vcf | tojson }},
  "TrainGenotypeFilteringModel.output_prefix": {{ test_batch.name | tojson }},
  "TrainGenotypeFilteringModel.gq_recalibrator_model_file": {{ reference_resources.aou_recalibrate_gq_model_file | tojson }},
  "TrainGenotypeFilteringModel.truth_json": {{ test_batch.recalibrate_gq_truth_json | tojson }},

  "TrainGenotypeFilteringModel.recalibrate_gq_args": [
    "--keep-homvar false",
    "--keep-homref true",
    "--keep-multiallelic true",
    "--skip-genotype-filtering true",
    "--min-samples-to-estimate-allele-frequency -1"
  ],
  "TrainGenotypeFilteringModel.genome_tracks": [
    {{ reference_resources.recalibrate_gq_genome_track_repeatmasker | tojson }},
    {{ reference_resources.recalibrate_gq_genome_track_segdup | tojson }},
    {{ reference_resources.recalibrate_gq_genome_track_simple_repeats | tojson }},
    {{ reference_resources.recalibrate_gq_genome_track_umap100 | tojson }},
    {{ reference_resources.recalibrate_gq_genome_track_umap24 | tojson }}
  ],

  "TrainGenotypeFilteringModel.linux_docker": {{ dockers.linux_docker | tojson }},
  "TrainGenotypeFilteringModel.gatk_docker": {{ dockers.gq_recalibrator_docker | tojson }},
  "TrainGenotypeFilteringModel.sv_base_mini_docker": {{ dockers.sv_base_mini_docker | tojson }},
  "TrainGenotypeFilteringModel.sv_pipeline_docker": {{ dockers.sv_pipeline_docker | tojson }}
}
